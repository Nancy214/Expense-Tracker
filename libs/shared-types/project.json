{
    "name": "shared-types",
    "$schema": "../../node_modules/nx/schemas/project-schema.json",
    "sourceRoot": "libs/shared-types/src",
    "projectType": "library",
    "targets": {
        "build": {
            "executor": "nx:run-commands",
            "outputs": ["{workspaceRoot}/libs/shared-types/dist"],
